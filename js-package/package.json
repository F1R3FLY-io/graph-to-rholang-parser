{
  "name": "@f1r3fly-io/graphl-parser",
  "version": "0.0.8",
  "description": "Parser for GraphL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F1R3FLY-io/graph-to-rholang-parser.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "main": "./dist/graphl-parser.cjs.js",
  "module": "./dist/graphl-parser.es.js",
  "browser": "dist/graphl-parser.umd.js",
  "types": "./dist/graphl-parser.d.ts",
  "exports": {
    ".": {
      "types": "./dist/graphl-parser.d.ts",
      "import": "./dist/graphl-parser.es.js",
      "require": "./dist/graphl-parser.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-static-copy": "^3.1.2"
  }
}
